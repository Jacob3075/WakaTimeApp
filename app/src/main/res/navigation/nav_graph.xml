<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/nav_graph"
    app:startDestination="@id/loginPage">
    <fragment
        android:id="@+id/loginPage"
        android:name="com.jacob.wakatimeapp.login.ui.LoginPage"
        android:label="login_page_fragment" >
        <action
            android:id="@+id/loginPage_to_homePage"
            app:destination="@id/homePage"
            app:popUpTo="@id/nav_graph"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/homePage"
        android:name="com.jacob.wakatimeapp.home.ui.HomePage"
        android:label="home_page_fragment" />
</navigation>
